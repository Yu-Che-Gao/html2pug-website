    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "canvas": "^1.3.10",
    "domify": "^1.4.0",
    "express": "^4.13.4",
    "html2jade": "^0.8.4",
    "pug": "^0.1.0",
    "pug-cli": "0.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
